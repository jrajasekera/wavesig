<div id="new-file-upload-container">
  <a href=<%= user_dashboard_path %> class="btn btn-secondary btn-sm back-btn">
    <div class="collapse-arrow" ><i class="arrow left"></i> Back to Dashboard</div>
  </a>
  <h1>Upload an audio file</h1>
  <%= form_for @uploadedfile, url: create_upload_file_path, html: { method: :post } do |f| %>

    <div class="field text-field">
<!--      <%#= f.label :fileName %><br />-->
      <%= f.text_field :fileName, :placeholder => 'File Name', :class => 'text-input' %>
    </div>

    <div class="field text-field">
<!--      <%#= f.label :description %><br />-->
      <%= f.text_field :description, :placeholder => 'Description', :class => 'text-input' %>
    </div>

    <div class="field browse-file-field">
<!--      <%#= f.label "Audio File" %><br />-->
      <%= f.file_field :audio_file %>
    </div>

    <div class="actions submit-btn">
      <%= f.submit "Upload File", :class => 'btn btn-primary btn-block' %>
    </div>
  <% end %>
</div>