<h3 class="page-title">Find Origin Results</h3>
<table class="table table-dark table-striped shadow rounded">
  <thead>
    <tr>
      <th scope="col">Analysis Time</th>
      <th scope="col">Origin User</th>
    </tr>
  </thead>
  <tbody>
  <% @originJobs.each do |result| %>
    <tr>
      <td>Now</td>
      <td>
        <div class="spinner-border text-primary" role="status">
          <span class="sr-only">Loading...</span>
        </div>
      </td>
    </tr>
  <% end %>

  <% @originResults.each do |result| %>
    <tr>
      <td><%= local_time(result[0], '%m/%d/%Y %I:%M %p') %></td>
      <td><%= result[1] %></td>
    </tr>
  <% end %>

  <% if @originResults.length == 0 && @originJobs.length == 0 %>
    <tr>
      <td>N/A</td>
      <td>N/A</td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= content_for :render_async %>